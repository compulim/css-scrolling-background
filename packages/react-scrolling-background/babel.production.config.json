{
  "ignore": [
    "**/*.spec.js",
    "**/*.spec.jsx",
    "**/*.spec.ts",
    "**/*.spec.tsx",
    "**/*.test.js",
    "**/*.test.jsx",
    "**/*.test.ts",
    "**/*.test.tsx"
  ],
  "plugins": [
    [
      "babel-plugin-transform-inline-environment-variables",
      {
        "include": ["npm_package_name", "npm_package_version"]
      }
    ]
  ]
}
